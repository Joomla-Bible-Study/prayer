<?xml version="1.0" encoding="utf-8"?><form>	<fieldset addfieldpath="/administrator/components/com_cwmprayer/models/fields">		<field name="id" type="text" default="0" label="JGLOBAL_FIELD_ID_LABEL"		       readonly="true" class="readonly"		       description="JGLOBAL_FIELD_ID_DESC"/>		<field name="name" type="text" class="inputbox"		       size="60" label="COM_CWMPRAYER_NAME"		       description="COM_CWMPRAYER_NAME_DESC" required="true"/>		<field name="published" type="list" label="JSTATUS"		       description="JFIELD_PUBLISHED_DESC" class="chzn-color-state" default="1">			<option value="1">Yes</option>			<option value="0">No</option>		</field>		<field name="feed" type="url" class="inputbox"		       size="60" label="Feed"		       description="" required="true"/>		<field name="target" type="LinkTarget"		       label="Target" description=""		       class="inputbox" default=""/>		<field name="ordering" type="text" label="JFIELD_ORDERING_LABEL"		       description="JFIELD_ORDERING_DESC" size="6"		       default="0" />	</fieldset>	<fieldset addfieldpath="/administrator/components/com_categories/models/fields">		<field name="catid"		       type="categoryedit"		       label="JCATEGORY"		       description="JFIELD_CATEGORY_DESC"		       extension="COM_CWMPRAYER"		       class="inputbox" default=""/>	</fieldset></form>